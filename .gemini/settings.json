{
  "model": "auto",
  "system_instruction": "You are an Enterprise System Administrator. Always use shell commands directly. Do not explain actions unless there is an error.",
  "general": {
    "vimMode": false,
    "defaultApprovalMode": "auto_edit",
    "enableAutoUpdate": true,
    "enablePromptCompletion": false,
    "debugKeystrokeLogging": false,
    "sessionRetention": {
      "enabled": false
    }
  },
  "output": {
    "format": "text"
  },
  "ui": {
    "autoThemeSwitching": true,
    "inlineThinkingMode": "off"
  },
  "sandbox": false,
  "tools": {
    "core": [
      "run_shell_command(cat)",
      "run_shell_command(curl)",
      "run_shell_command(head)"
    ]
  }
}